<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/menuRightPlate"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_marginTop="0dp"
	android:layout_marginBottom="0dp"
	android:layout_marginLeft="0dp"
	android:layout_marginRight="0dp"
	android:paddingTop="0dp"
	android:paddingBottom="0dp"
	android:paddingLeft="0dp"
	android:paddingRight="0dp">
	<ImageView
		android:id="@+id/a"
		android:src="@drawable/menu_right_spaghetti"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		android:layout_marginLeft="5dp"
		android:layout_marginRight="0dp"
		android:layout_centerVertical="true"
		android:paddingTop="0dp"
		android:paddingBottom="0dp"
		android:paddingLeft="0dp"
		android:paddingRight="0dp"/>
	<ImageView
		android:src="@drawable/menu_right_7_location_web_site"
		android:layout_centerInParent="true"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		android:layout_marginLeft="0dp"
		android:layout_marginRight="0dp"
		android:layout_alignTop="@id/a"
		android:layout_alignBottom="@id/a"
		android:layout_alignLeft="@id/a"
		android:layout_alignRight="@id/a"
		android:paddingTop="0dp"
		android:paddingBottom="0dp"
		android:paddingLeft="0dp"
		android:paddingRight="0dp"
		android:scaleType="fitCenter"/>
	<EditText
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		android:layout_marginLeft="0dp"
		android:layout_marginRight="0dp"
		android:layout_toRightOf="@id/a"
		android:paddingTop="5dp"
		android:paddingBottom="5dp"
		android:paddingLeft="10dp"
		android:paddingRight="5dp"
		android:textSize="24sp"
		android:textStyle="normal"/>
</RelativeLayout>
